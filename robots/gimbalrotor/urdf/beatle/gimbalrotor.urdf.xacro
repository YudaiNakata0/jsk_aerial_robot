<?xml version="1.0"?>
<robot
    xmlns:xacro="http://www.ros.org/wiki/xacro" name="gimbalrotor_link" >

  <xacro:macro name="gimbalrotor_center_link" params="child1 child2 child3 child4">
    ## dummy
    <link name="root">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.00001"/>
        <inertia
            ixx="0.000001" ixy="0.0" ixz="0.0"
            iyy="0.000001" iyz="0.0"
            izz="0.000001"/>
      </inertial>
    </link>
    <joint name="root_joint" type="fixed">
      <parent link="root"/>
      <child link="base_link"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
    </joint>

    ## link
    <link name="base_link">
      <inertial>
        <origin xyz="0.00859922 -0.00054638 0.08218887" rpy="0 0 0"/>
        <mass value="2.24223241"/>
        <inertia
          ixx="0.06463170"
          ixy="-0.00016812"
          ixz="0.00453111"
          iyy="0.08594030"
          iyz="0.00005885"
          izz="0.09711048"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0.0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://gimbalrotor/urdf/beatle/mesh/base_link_withpen.STL"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.518 0.518 0.241"/>
        </geometry>
      </collision>
    </link>

    <xacro:damping_factor link="base_link"/>

    <link name="fc">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.00001"/>
        <inertia
            ixx="0.000001" ixy="0.0" ixz="0.0"
            iyy="0.000001" iyz="0.0"
            izz="0.000001"/>
      </inertial>
    </link>
    <joint name="fc_joint" type="fixed">
      <child link="fc"/>
      <parent link="base_link"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
    </joint>

    <link name="end_effector">
      <inertial>
	<origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.00001"/>
        <inertia
            ixx="0.000001" ixy="0.0" ixz="0.0"
            iyy="0.000001" iyz="0.0"
            izz="0.000001"/>
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.018 0.018 0.163"/>
        </geometry>
      </collision>
    </link>
    <joint name="base_end_effector_joint" type="fixed">
      <child link="end_effector"/>
      <parent link="base_link"/>
      <origin rpy="0 0 0" xyz="0.337 0 0.195"/>
      <axis xyz="0 0 1"/>
    </joint>

    ## rotor arms
    <link name="rotor_arm${child1}">
      <inertial>
        <origin xyz="-0.01328822 -0.00018083 -0.02299702" rpy="0 0 0"/>
        <mass value="0.05661792"/>
        <inertia
          ixx="0.00007282"
          ixy="0.00000017"
          ixz="0.00002009"
          iyy="0.00008228"
          iyz="0.00000018"
          izz="0.00002219"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://gimbalrotor/urdf/beatle/mesh/rotor_arm.STL"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.131 0.0325 0.065"/>
        </geometry>
      </collision>
    </link>
    <joint name="base_arm_joint${child1}" type="fixed">
      <parent link="base_link"/>
      <child link="rotor_arm${child1}"/>
      <origin rpy="0 0 ${pi*5.0/4.0}" xyz="-0.1204 -0.1204 0.0555"/>
      <axis xyz="0 0 1"/>
    </joint>

    <link name="rotor_arm${child2}">
      <inertial>
        <origin xyz="-0.01328822 -0.00018083 -0.02299702" rpy="0 0 0"/>
        <mass value="0.05661792"/>
        <inertia
          ixx="0.00007282"
          ixy="0.00000017"
          ixz="0.00002009"
          iyy="0.00008228"
          iyz="0.00000018"
          izz="0.00002219"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://gimbalrotor/urdf/beatle/mesh/rotor_arm.STL"/>
        </geometry>
      </visual>
    </link>
    <joint name="base_arm_joint${child2}" type="fixed">
      <parent link="base_link"/>
      <child link="rotor_arm${child2}"/>
      <origin rpy="0 0 ${pi*7.0/4.0}" xyz="0.1204 -0.1204 0.0555"/>
      <axis xyz="0 0 1"/>
    </joint>

    <link name="rotor_arm${child3}">
      <inertial>
        <origin xyz="-0.01328822 -0.00018083 -0.02299702" rpy="0 0 0"/>
        <mass value="0.05661792"/>
        <inertia
          ixx="0.00007282"
          ixy="0.00000017"
          ixz="0.00002009"
          iyy="0.00008228"
          iyz="0.00000018"
          izz="0.00002219"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://gimbalrotor/urdf/beatle/mesh/rotor_arm.STL"/>
        </geometry>
      </visual>
    </link>
    <joint name="base_arm_joint${child3}" type="fixed">
      <parent link="base_link"/>
      <child link="rotor_arm${child3}"/>
      <origin rpy="0 0 ${pi*3.0/4.0}" xyz="-0.1204 0.1204 0.0555"/>
      <axis xyz="0 0 1"/>
    </joint>

    <link name="rotor_arm${child4}">
      <inertial>
        <origin xyz="-0.01328822 -0.00018083 -0.02299702" rpy="0 0 0"/>
        <mass value="0.05661792"/>
        <inertia
          ixx="0.00007282"
          ixy="0.00000017"
          ixz="0.00002009"
          iyy="0.00008228"
          iyz="0.00000018"
          izz="0.00002219"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://gimbalrotor/urdf/beatle/mesh/rotor_arm.STL"/>
        </geometry>
      </visual>
    </link>
    <joint name="base_arm_joint${child4}" type="fixed">
      <parent link="base_link"/>
      <child link="rotor_arm${child4}"/>
      <origin rpy="0 0 ${pi/4.0}" xyz="0.1204 0.1204 0.0555"/>
      <axis xyz="0 0 1"/>
    </joint>

    <xacro:friction self="base_link"/>
  </xacro:macro>

  <xacro:macro name="gimbalrotor_thrust_link" params="self rotor_direction">
    ## link
    #### rotor parent link (virtual) ####
    <link name="gimbal_link${self}">
      <inertial>
        <origin xyz="0.02375221 0.00000914 0.00776312" rpy="0 0 0"/>
        <mass value="0.15944"/>
        <inertia
          ixx="0.00008414"
          ixy="-0.00000003"
          ixz="0.00003024"
          iyy="0.00017278"
          iyz="0.00000001"
          izz="0.00016748"/>
      </inertial>
      <collision>
        <origin xyz="0.0 0 0" rpy="0 0 0"/>
        <geometry>
          <cylinder radius="0.018" length="0.033"/>
          </geometry>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://gimbalrotor/urdf/beatle/mesh/rotor_module.STL"/>
        </geometry>
      </visual>
    </link>
    <xacro:damping_factor link="gimbal_link${self}"/>

    ## rotor parent link (virtual)
    <link name="rotor_parent${self}">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.00001"/>
        <inertia
            ixx="0.000001" ixy="0.0" ixz="0.0"
            iyy="0.000001" iyz="0.0"
            izz="0.000001"/>
      </inertial>
    </link>

    ## thrust link (virtual)
    <link name="thrust${self}">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.00001"/>
        <inertia
            ixx="0.001" ixy="0.0" ixz="0.0"
            iyy="0.001" iyz="0.0"
            izz="0.001"/>
      </inertial>
    </link>
    <xacro:damping_factor link="thrust${self}"/>

    #### joints ####

    #### gimbal joints ####
    <joint name="gimbal${self}" type="revolute">
      <limit effort="6.6" lower="-2.0" upper="2.0" velocity="3.0"/>
      <parent link="rotor_arm${self}"/>
      <child link="gimbal_link${self}"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <axis xyz="1 0 0"/>
      <dynamics damping="0.8" friction="0.0"/>
    </joint>

    <transmission name="joint_tran${self}">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="gimbal${self}">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      </joint>
      <actuator name="servo${self}">
        <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>
    #### rotor parent link joint (virtual) ####
    <joint name="rotor_parent_link_joint${self}" type="fixed">
      <parent link="gimbal_link${self}"/>
      <child link="rotor_parent${self}"/>
      <origin rpy="0 0 0" xyz="0.018 0.00 0.00438"/>
      <axis xyz="0 0 ${rotor_direction}"/>
    </joint>

    #### rotor joint (virtual) ####
    <joint name="rotor${self}" type="continuous">
      <limit effort="100.0" lower="${min_force}" upper="${max_force}" velocity="0.5"/>
      <parent link="rotor_parent${self}"/>
      <child link="thrust${self}"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <axis xyz="0 0 ${rotor_direction}"/>
    </joint>

    <transmission name="rotor_tran${self}">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="rotor${self}">
        <hardwareInterface>RotorInterface</hardwareInterface>
      </joint>
      <actuator name="rotor_actuator${self}">
        <hardwareInterface>RotorInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

  </xacro:macro>
</robot>
